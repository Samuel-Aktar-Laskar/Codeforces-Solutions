{
  "manifest_version":3,
  "version": "1.0",
  "name": "Codeforces Solutions",
  "description": "Want to post your solution and read others solution like in LeetCode? This extension is all you need.",
  "permissions": [
    "activeTab",
    "https://codeforces.com/*"
  ],
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://codeforces.com/*"],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ]
}
